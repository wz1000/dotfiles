*dynamicColors: on

!urxvt color scheme:

*foreground: #D0D0D0
! 234 dark gray
URxvt*background: [99]#1c1c1c
! blacks?
*color0: #2e3436
*color8: #555753
! reds
*color1: #bf3f3f
*color9: #cd0000
! greens
! 154 bright green
*color2: #afff00
! 154 bright green
*color10: #afff00
! yellows
! 222 Light tan
*color3: #ffff87
! 208 Bright Orange
*color11: #ff8700
! blues
! 141 light purple
*color4: #AF87FF
! 141 light purple
*color12: #AF87FF
! magentas
! 197 Pink
*color5: #ff005f
! 125 plumb
*color13: #af005f
! cyans
*color6: #06989a
*color14: #66d9ef
*color7: #e5e5e5
*color15: #e5e5e5

!urxvt extensions:

URxvt.perl-ext-common:  default,matcher,tabbedex,font-size,vtwheel,fullscreen,selection-to-clipboard,osc-xterm-clipboard

!enabling clickable links:

URxvt.url-launcher:      /usr/bin/firefox
URxvt.matcher.button:   1

!urxvt scrolling options and cursor style:

URxvt*saveLines: 12000
URxvt*scrollstyle:plain
URxvt*cursorBlink: true

! urxvt tabs settings:

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 2
URxvt.tabbed.tab-bg: 8
URxvt.tabbed.autohide:  yes
URxvt.tabbed.new-button:  no
URxvt.tabbed.title: no

URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal

Xft.dpi: 96
Xft.antialias: 1
Xft.hinting:    1
Xft.hintstyle:  hintfull
Xft.rgba:       rgb
xterm*faceName: Liberation Mono
xterm*faceSize: 7 7 7 7 7 7 7 
URxvt*font: xft:Liberation Mono for Powerline-8
URxvt*boldFont: xft:Liberation Mono:bold
URxvt.letterSpace: -1

URxvt.urgentOnBell: true
